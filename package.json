{
  "name": "@statebacked/client",
  "version": "0.1.0",
  "description": "Client for the StateBacked.dev machine definitions for the State Backed XState backend as a service",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc src/index.ts --strict --declaration --module commonjs --outDir lib && tsc src/index.ts --strict --declaration --module esnext --outDir es",
    "openapi": "openapi --input ./api-spec/statebacked.openapi.v3.yaml --output gen --client fetch --useOptions --indent 2",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src",
    "doc": "typedoc"
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "es/**/*.js",
    "es/**/*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/statebacked/machine-def.git"
  },
  "keywords": [
    "statechart",
    "state machine",
    "scxml",
    "state",
    "finite state machine",
    "state backed",
    "backend as a service",
    "paas"
  ],
  "author": "Adam Berger <adam@statebacked.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/statebacked/machine-def/issues"
  },
  "homepage": "https://github.com/statebacked/machine-def#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "openapi-typescript-codegen": "^0.24.0",
    "prettier": "^2.8.8",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.6"
  }
}
